{
  "networks": {},
  "canisters": {
    "backend": {
      "candid": "backend/backend.did",
      "package": "backend",
      "type": "custom",
      "optimize": "cycles",
      "build": [
        "cargo build --target wasm32-unknown-unknown --release -p backend",
        "bash ./scripts/generate_did.sh",
        "bash ./scripts/rename_did.sh"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/backend.wasm",
      "metadata": [
        {
          "name": "candid:service",
          "visibility": "public"
        }
      ]
    },
    "frontend": {
      "type": "assets",
      "source": [
        "dist/"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      }
    },
    "pred": {
      "candid": "pred/pred.did",
      "package": "pred",
      "type": "custom",
      "optimize": "cycles",
      "build": [
        "export RUSTFLAGS='--cfg getrandom_backend=\"custom\"'",
        "RUSTFLAGS='--cfg getrandom_backend=\"custom\"' cargo build --target wasm32-unknown-unknown --release -p pred"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/pred.wasm"
    },
    "token_transfer_from_backend": {
      "candid": "src/token_transfer_from_backend/token_transfer_from_backend.did",
      "package": "token_transfer_from_backend",
      "type": "rust"
    },
    "icrc1_ledger_canister": {
      "type": "custom",
      "candid": "https://raw.githubusercontent.com/dfinity/ic/d87954601e4b22972899e9957e800406a0a6b929/rs/rosetta-api/icrc1/ledger/ledger.did",
      "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/ic-icrc1-ledger.wasm.gz"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
