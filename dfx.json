{
  "networks": {},
  "canisters": {
    "backend": {
      "candid": "backend/backend.did",
      "package": "backend",
      "type": "custom",
      "optimize": "cycles",
      "build": [
        "RUSTFLAGS='--cfg getrandom_backend=\"custom\"' cargo build --target wasm32-unknown-unknown --release -p backend",
        "bash ./scripts/generate_did.sh",
        "bash ./scripts/rename_did.sh"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/backend.wasm"
    },
    "frontend": {
      "type": "assets",
      "source": ["dist/"],
      "frontend": {
        "entrypoint": "dist/index.html"
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
