{
  "networks": {
  },
  "canisters": {
    "backend": {
      "candid": "src/backend/backend.did",
      "package": "backend",
      "type": "custom",
      "optimize": "cycles",
      "build": [
        "RUSTFLAGS='--cfg getrandom_backend=\"custom\"' cargo build --target wasm32-unknown-unknown --release -p backend"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/backend.wasm"
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "source": [
        "src/frontend/dist"
      ],
      "type": "assets",
      "workspace": "frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}